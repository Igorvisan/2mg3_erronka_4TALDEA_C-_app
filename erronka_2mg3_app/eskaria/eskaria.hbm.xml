<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="erronka_2mg3_app"
                   namespace="erronka_2mg3_app.eskaria">

	<class name="erronka_2mg3_app.eskaria.Eskaera" table="eskaera">
		<id name="Id" column="id" type="int">
			<generator class="identity" />
		</id>

		<!-- Los nombres de las propiedades deben coincidir con los nombres de tu tabla de la base de datos en la propiedad column -->
		<many-to-one name="MahaiaId" column="mahaia_id" class="erronka_2mg3_app.mahaia.mahaia" not-null="true"/>
		<many-to-one name="LangileId" column="langile_id" class="erronka_2mg3_app.langileLogIn" not-null="true"/>

		<property name="Totala" column="totala" type="double"/>
		<property name="Ordainduta" column="ordainduta" type="bool"/>
		<property name="FakturaPath" column="faktura_path" type="string"/>

	</class>
</hibernate-mapping>
