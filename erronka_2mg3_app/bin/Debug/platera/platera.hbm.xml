<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="erronka_2mg3_app"
                   namespace="erronka_2mg3_app.platera">

	<!-- more mapping info here -->
	<class name="erronka_2mg3_app.platera.platera" table="platera">
		<id name="Id" column="id" type="int">
			<generator class="identity" />
		</id>
		<!-- Los nombres de la propiedades deben de coincidir con los nombres de tu tabla de la base de datos en la propiedad column -->
		<property name="Izena" column="izena" type="string"/>
		<property name="PrestaketaDenbora" column="prestaketa_denbora" type="int"/>
		<property name="Prezioa" column="prezioa" type="double"/>
		<property name="PlaterMota" column="plater_mota" type="string"/>
	</class>
</hibernate-mapping>
