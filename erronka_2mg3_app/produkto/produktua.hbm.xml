<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="erronka_2mg3_app"
                   namespace="erronka_2mg3_app.produkto">

	<!-- more mapping info here -->
	<class name="erronka_2mg3_app.produkto.produktua" table="produktua">
		<id name="Id" column="id" type="int">
			<generator class="identity" />
		</id>
		<!-- Los nombres de la propiedades deben de coincidir con los nombres de tu tabla de la base de datos en la propiedad column -->
		<property name="Izena" column="izena" type="string"/>
		<property name="Egoera" column="egoera" type="string"/>
		<property name="GutxienezkoKantitatea" column="gutxienezko_kantitatea" type="int"/>
		<property name="OraingoKantitatea" column="oraingo_kantitatea" type="int"/>
		<property name="GehienezkoKantitatea" column="gehienezko_kantitatea" type="int"/>
	</class>
</hibernate-mapping>
